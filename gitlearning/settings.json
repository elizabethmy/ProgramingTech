{
    "workbench.colorTheme": "Default Dark+",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "workbench.editor.wrapTabs": true,
    "background.enabled": true,
    "background.loop": false,
    "background.useDefault": false,
    "background.useFront": false,
    "background.style": {
          "content": "''",
          "pointer-events": "none",
          "position": "absolute",
          "z-index": "99999",
          "background-position": "right",
          "background-size": "cover",
          "background-repeat": "no-repeat",
          "opacity": 0.1
      },
    "background.customImages": [
          "file:///C:/Users/lethi/OneDrive/Pictures/Saved%20Pictures/ml.png",
    ],
    "editor.minimap.enabled": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.renderWhitespace": "all",
    "code-eol.crlfCharacter": "CLRF",
    "code-eol.highlightExtraWhitespace": true,
    "code-eol.newlineCharacter": "LF",
    "code-eol.returnCharacter": "CR",
    "code-eol.highlightNonDefault": true,
    "[markdown]": {
        "editor.defaultFormatter": "fcrespo82.markdown-table-formatter"
    },
    "search.quickOpen.history.filterSortOrder": "recency",
    "search.quickOpen.includeSymbols": true,
    "search.showLineNumbers": true,
    "search.seedWithNearestWord": true,
    "search.seedOnFocus": true,
    "search.sortOrder": "type",
    "workbench.iconTheme": "vscode-icons",
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.scrollback": 90000,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "bracketPairColorizer.rulerPosition": "Full",
    "bracketPairColorizer.showBracketsInRuler": true,
    "bracketPairColorizer.highlightActiveScope": true,
    "doxdocgen.file.fileOrder": [
        "file",
        "author",
        "brief",
        "version",
        "date",
        "empty",
        "copyright",
        "empty",
        "custom"
    ],
    "doxdocgen.generic.authorName": "my.lethidiem",
    "doxdocgen.generic.authorTag": "@author {my.lethidiem} ({lethidiemmy961996@gmail.com})",
    "gitlens.fileAnnotations.command": "blame",
    "gitlens.advanced.fileHistoryFollowsRenames": true,
    "gitlens.advanced.fileHistoryShowAllBranches": true,
    "gitlens.advanced.commitOrdering": "date",
}